#Write a prpgram to read a list of elements
#create a new list that holds all the elements  minus the duplicates
def remove_duplicates(numbers):	#def is used for defining the function
	newlist= []
	for number in numbers:
		if number not in newlist:
			newlist.append(number)
	
	print(newlist);
	return newlist		#function ends
remove_duplicates([1,2,3,4,5,5,3,2])
